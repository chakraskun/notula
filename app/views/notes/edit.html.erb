                                <!--begin::Form Wizard-->
                                <%= form_with model: @note,
                  url: notes_path,
                  class: 'form fv-plugins-bootstrap fv-plugins-framework',
                  id: 'kt_projects_add_form',
                  local:1 do |f|
                %>
                                <%= render partial: 'form', locals: {f:f} %>
